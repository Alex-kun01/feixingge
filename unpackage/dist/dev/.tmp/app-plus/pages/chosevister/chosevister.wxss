.content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.content .info_wrap {
  width: 90%;
  margin: 0 auto;
  background: #FFFFFF;
  border-radius: 5px;
  position: relative;
  margin-top: 4vw;
  padding: 3vw 0;
}
.content .info_wrap .chosego_wrap {
  color: #FF9805;
}
.content .info_wrap .item_wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  width: 100%;
  padding: 0vw 4vw;
  height: 10vw;
}
.content .info_wrap .item_wrap .rt_img {
  width: 10vw;
  height: 10vw;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
}
.content .info_wrap .item_wrap .num_wrap {
  color: #999999;
  font-weight: 200;
}
.content .info_wrap .item_wrap .name {
  line-height: 10vw;
  width: auto;
  text-align: center;
  font-size: 5vw;
}
.content .info_wrap .item_wrap .time_show {
  line-height: 18vw;
  width: auto;
  text-align: center;
}
.content .info_wrap .item_wrap .search_btn {
  width: 90%;
  height: 13vw;
  line-height: 13vw;
  border-radius: 10vw;
  background: #FF9805;
}
.content .info_wrap .btn_wrap {
  height: 18vw;
  margin-bottom: 6vw;
}
.content .info_wrap .go_next_icon {
  left: 90%;
  -webkit-transform: rotateZ(180deg);
          transform: rotateZ(180deg);
}
.content .info_wrap .go_next_icon image {
  width: 2vw !important;
  height: 4vw !important;
}
.content .icon_wrap {
  width: 10vw;
}
.content .icon_wrap image {
  width: 8vw !important;
  height: 8vw !important;
}
.content .go_center {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
}

