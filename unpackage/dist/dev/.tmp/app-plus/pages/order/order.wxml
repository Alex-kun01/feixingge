ap',[['changeTimeIndex',[2]]]]]}}" class="item" bindtap="__e"><view class="{{[(true)?'text':'',(tiemIndex.two)?'active_time':'']}}">出发 早 ——> 晚</view><image hidden="{{!(tiemIndex.two)}}" src="../../static/order/gou@2x.png" mode></image></view></view></block></view></view><view data-event-opts="{{[['tap',[['changesee',['$event']]]]]}}" class="see" bindtap="__e"><view class="{{[(true)?'radio':'',(isend)?'active':'']}}"><view class="show"></view></view><view class="text">查看有效订单</view></view></view><view class="main"><block wx:for="{{orderData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[(true)?'order_item':'',(item.orderState==2)?'active':'']}}"><view class="type-status"><view class="type"><block wx:if="{{item.ticketType.isGaotie}}"><view style="line-height:40rpx;display:flex;"><image style="width:40rpx;height:40rpx;margin-right:18rpx;" src="../../static/order/gaotie.png" mode></image><view>火车票</view></view></block><block wx:if="{{item.ticketType.isFly}}"><view style="line-height:40rpx;display:flex;"><image style="width:40rpx;height:40rpx;margin-right:18rpx;" src="../../static/order/fly.png" mode></image><view>飞机票</view></view></block><block wx:if="{{item.ticketType.isHotel}}"><view style="line-height:40rpx;display:flex;"><image style="width:40rpx;height:40rpx;margin-right:18rpx;" src="../../static/order/hotel.png" mode></image><view class="mColor">酒店</view></view></block></view><view class="status"><block wx:if="{{item.orderState==1}}"><text>待支付</text></block><block wx:if="{{item.orderState==2}}"><text class="mColor">已取消</text></block></view></view><view class="full"><block wx:if="{{item.ticketType.isGaotie||item.ticketType.isFly}}"><view class="distance-time"><text class="distance">成都东-->隆昌北</text><text class="time">1-19 07:30 实际出发</text></view></block><block wx:if="{{item.ticketType.isHotel}}"><view class="distance-time"><text class="distance mColor">{{item.hotel}}</text><text class="time mColor">1-19 07:30 共1晚</text></view></block><view 