.uni-popup {
  z-index: 999;
}
 .uni-popup__wrapper-box {
  position: relative;
  background: #ffffff;
}
.item_wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  width: 100%;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 4vw;
  height: 18vw;
}
.item_wrap .rt_img {
  width: 10vw;
  height: 10vw;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
}
.item_wrap .name {
  line-height: 18vw;
  width: auto;
  text-align: center;
  font-size: 5vw;
}
.item_wrap .see_gt_radio {
  position: absolute;
  line-height: 1;
  top: 50%;
  left: 30%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.item_wrap .time_show {
  line-height: 18vw;
  width: auto;
  text-align: center;
}
.item_wrap .search_btn {
  width: 90%;
  height: 13vw;
  line-height: 13vw;
  border-radius: 10vw;
  background: #FF9805;
}
.item_wrap .history_tips {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.item_wrap .history_tips .tips_wrap {
  background: #F4F4F4;
  padding: 1vw 2vw;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-radius: 5vw;
  margin: 1vw 0 0 1vw;
}
.item_wrap .history_tips .tips_wrap .img_wrap {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column;
          flex-flow: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.item_wrap .history_tips .tips_wrap .img_wrap image {
  width: 4vw;
  height: 4vw;
}
.item_wrap .history_tips .tips_wrap .text_wrap {
  font-size: 4vw;
  margin-left: 1vw;
  height: 7vw;
  line-height: 7vw;
  font-weight: 200;
  color: #666666;
}
.item_wrap .history_tips .tips_wrap .clear_history {
  width: 12vw;
  text-align: center;
  letter-spacing: 1vw;
}

